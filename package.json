{
  "name": "@noshiro/discord-poll-app",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "buildw": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "fmt": "prettier --ignore-path .prettierignore  --write .",
    "setup": "echo '{ \"polls\": [] }' > ./db.json",
    "start": "node ./dist/index.js",
    "start:dev": "export NODE_ENV='development' && node ./dist/index.js",
    "test": "echo 'no test script'",
    "heroku:bash": "heroku run bash -a poll-discord-app"
  },
  "dependencies": {
    "@noshiro/ts-utils": "1.0.24",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "pg": "^8.5.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.9",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=12.x.x"
  }
}
